{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "com.webservdev.app",
  "name": "WebServDev",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/www/",
  "url": "/index.html",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "os.*",
    "filesystem.*",
    "window.*",
    "app.*"
  ],
  "globalVariables": {},
  "modes": {
    "window": {
      "title": "WebServDev",
      "width": 1200,
      "height": 800,
      "minWidth": 800,
      "minHeight": 600,
      "enableInspector": true,
      "openInspectorOnStartup": true,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "WebServDev",
    "resourcesPath": "./www/",
    "extensionsPath": "./extensions/",
    "clientLibrary": "./www/neutralino.js",
    "binaryVersion": "6.4.0",
    "clientVersion": "6.4.0",
    "copyItems": [
      "./bootstrap.html",
      "./index.html"
    ]
  }
}